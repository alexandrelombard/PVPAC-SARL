package car.events

import model.geom.Point2d
import io.sarl.lang.core.Address

/** 
 * @author alombard
 * 
 */
event PositionChanged {
	val position : Point2d
	val heading : double
	
	public new(source : Address, position : Point2d, heading : double) {
		super(source)
		
		this.position = position
		this.heading = heading
	}
}
