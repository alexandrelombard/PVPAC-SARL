package model.geom

class Point2d extends IPoint2d {
	public var x : double
	public var y : double
	
	public new() {
		x = 0
		y = 0
	}
	
	public new(x : double, y : double) {
		this.x = x
		this.y = y
	}
	
	override getX : double {
		x
	}

	override getY : double {
		y
	}
	
	public def setX(x : double) {
		this.x = x
	}
	
	public def setY(y : double) {
		this.y = y
	}
	
}